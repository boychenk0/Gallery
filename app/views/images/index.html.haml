.container-fluid
  .row-fluid
    .span3
      -cache("categories_content/#{user_signed_in? ? current_user.cache_key : ''}") do
        =render 'categories/categories_menu'
    .span9
      #title_images
        =t('.all_image')
      #images
        #images-sorted
          -#sorted by
          -#-#=link_to 'created', root_path
          -#-#=link_to 'likes', root_path
          -#-#=link_to 'comments', root_path
        -cache("images_list/all_images") do
          #images-list
            =render 'images/images_list'
          .btn-group.paginate
            = paginate @images, :remote => true, :window => 1



